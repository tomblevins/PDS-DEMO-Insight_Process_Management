<?xml version="1.0" encoding="UTF-8"?><record_update sys_domain="global" table="sys_script_client">
    <sys_script_client action="INSERT_OR_UPDATE">
        <active>true</active>
        <applies_extended>false</applies_extended>
        <condition/>
        <description>Script to hide Processing Info tab if User IS NOT admin</description>
        <field/>
        <global>true</global>
        <isolate_script>true</isolate_script>
        <messages/>
        <name>Hide Processing Info Tab </name>
        <order/>
        <script><![CDATA[function onLoad() {
    //Type appropriate comment here, and begin script below
    var id = (g_form.getUniqueValue());
    if (g_user.userID == id || g_user.hasRole('admin')) {
        g_form.setSectionDisplay('Processing Info', true);
    } else {
        g_form.setSectionDisplay('Processing Info', false);
    }
}]]></script>
